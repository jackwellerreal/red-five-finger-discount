{
    "manifest_version": 3,
    "name": "RRFFD",
    "description": "The infamous Five Finger Discount",
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "version": "1.0.0",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["https://www.redrooster.com.au/*"],
    "action": {
        "default_title": "Modify Cart",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}
